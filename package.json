{
	"name": "soybean-admin",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"dev:prod": "cross-env VITE_HTTP_ENV=prod vite",
		"typecheck": "vue-tsc",
		"build": "npm run typecheck && cross-env VITE_HTTP_ENV=prod vite build",
		"build:test": "npm run typecheck && cross-env VITE_HTTP_ENV=test vite build",
		"build:vercel": "cross-env VITE_HTTP_ENV=prod VITE_IS_VERCEL=1 vite build",
		"preview": "vite preview --port 5050",
		"release": "standard-version",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
		"lint": "eslint --fix ./ --ext .vue,.js,jsx,.ts,tsx",
		"prepare": "husky install",
		"postinstall": "patch-package"
	},
	"lint-staged": {
		"*.{vue,js,jsx,ts,tsx}": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	},
	"dependencies": {
		"@antv/g2plot": "^2.4.8",
		"@better-scroll/core": "^2.4.2",
		"@tauri-apps/api": "^1.2.0",
		"@vueuse/core": "^7.6.2",
		"axios": "^0.26.0",
		"clipboard": "^2.0.10",
		"colord": "^2.9.2",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.10.7",
		"form-data": "^4.0.0",
		"lodash-es": "^4.17.21",
		"naive-ui": "^2.25.3",
		"pinia": "^2.0.11",
		"print-js": "^1.6.0",
		"qs": "^6.10.3",
		"soybean-admin-layout": "^1.0.3",
		"swiper": "^8.0.6",
		"ua-parser-js": "^1.0.2",
		"vditor": "^3.8.11",
		"vue": "^3.2.31",
		"vue-router": "^4.0.12",
		"wangeditor": "^4.7.11",
		"xgplayer": "^2.31.4"
	},
	"devDependencies": {
		"@amap/amap-jsapi-types": "^0.0.8",
		"@commitlint/cli": "^16.2.1",
		"@commitlint/config-conventional": "^16.2.1",
		"@iconify/json": "^2.1.4",
		"@iconify/vue": "^3.1.3",
		"@tauri-apps/cli": "^1.2.3",
		"@types/bmapgl": "^0.0.5",
		"@types/crypto-js": "^4.1.1",
		"@types/lodash-es": "^4.17.7",
		"@types/node": "^17.0.19",
		"@types/qs": "^6.9.7",
		"@types/ua-parser-js": "^0.7.36",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vitejs/plugin-vue": "^2.2.2",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^10.0.0",
		"commitizen": "^4.2.4",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^6.3.0",
		"eslint": "^8.9.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.5.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.4",
		"mockjs": "^1.1.0",
		"patch-package": "^6.4.7",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.5.1",
		"rollup-plugin-visualizer": "^5.5.4",
		"sass": "^1.49.8",
		"typescript": "^4.5.5",
		"unplugin-icons": "^0.13.2",
		"unplugin-vue-components": "^0.17.19",
		"vite": "2.8.4",
		"vite-plugin-html": "^3.0.6",
		"vite-plugin-mock": "^2.9.6",
		"vite-plugin-windicss": "^1.7.1",
		"vue-tsc": "^0.31.4",
		"vueuc": "^0.4.26",
		"windicss": "^3.4.4"
	}
}
